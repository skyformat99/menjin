  {
    //DelayN10ms(500);    
    //WaitConnect();
  }
  /*******/
  cmdmode=0;
  if(FactorSign==0)
    Connect_Service=1;
  else
    Connect_Service=0;
  
  //SysTick_Config(SYSTIME);
  
  bFlagOK=0;
#if 1
  OffLineTime=OFF_LINE_TIME;
  TestHobTime=100;
  while(1)
  {
    /* 1、Hand end telecontrol*/
    if(Key_end_Telecontrol==1)
    {
      StartTime=0;
      Key_end_Telecontrol=0;
      Speak(1);
      Finish=1;
      FinishTime=0; 
      Keyend=1;
      
      if(OrderOperState!=0)
      {
        OrderOperState=2;
        DelayN10ms(50);
        bFlagOK=0;
        //memset(CommRcvBuf,0,512);
