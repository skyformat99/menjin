                    }                    

                    DelayN10ms(50);
                    bFlagOK=0;
                    DataReturn(7);
                    DelayNum10ms=200;
                    while(bFlagOK==0)
                    {
                      if(DelayNum10ms==0)
                        break;
                    }
                    DelayNum10ms=0;
                    if(bFlagOK==1)
                    {
                      OffLineTime=OFF_LINE_TIME;
