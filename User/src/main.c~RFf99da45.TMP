              Rcv_OrderWeek=Rcv_OrderWeek|((tmp[2]-0x30)<<2);
              Rcv_OrderWeek=Rcv_OrderWeek|((tmp[3]-0x30)<<3);
              Rcv_OrderWeek=Rcv_OrderWeek|((tmp[4]-0x30)<<4);
