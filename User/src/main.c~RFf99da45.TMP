          else if((Warmtemp-WaterTemp)>2)
            WaterTemp=Warmtemp-2;
          else
            WaterTemp=Warmtemp-1;  
        }
      }
            
      else if(EnCool==1)  /*冷却*/
      {
        Mystate=4;
        if(Warm==1)
          WarmTime=TeleWarmTime*60;      
        
        if(WaterTemp<=Warmtemp)
        {
          WaterTemp=Warmtemp;
          EnCool=0;   
          if(Warm==0)
          {
                Speak(1);
                Finish=1;
                FinishTime=0;
              
                if(Connect_Service==0)
                  DataReturn(4);
                Mystate=0;
                StateTime=0;
                
                //RecordTable[RecordNum].endtime=;
                  LastTime[0]=0x32;
                  LastTime[1]=0x30;
