      LastTime[5]=0x30 + TMon%10;
      LastTime[6]=0x30 + TDay/10;
      LastTime[7]=0x30 + TDay%10;
      LastTime[8]=RecordTable[RecordNum].endtime[0]=THour/10 + 0x30;
      LastTime[9]=RecordTable[RecordNum].endtime[1]=THour%10 + 0x30;
      LastTime[10]=RecordTable[RecordNum].endtime[2]=TMin/10 + 0x30;
      LastTime[11]=RecordTable[RecordNum].endtime[3]=TMin%10 + 0x30;
      LastTime[12]=RecordTable[RecordNum].endtime[4]=TSec/10 + 0x30;
      LastTime[13]=RecordTable[RecordNum].endtime[5]=TSec%10 + 0x30;
      LastTime[14]=RecordTable[RecordNum].endtime[6]=0;
      RecordTable[RecordNum].energy=((RelayOnTime/100)*TEAPOTPOWER)/3600;
      
      RecordNum++;
      if(RecordNum>10)
        RecordNum=9;      
      Rst_Value();
      RELAY(0);
      Warm=0;
      WARM_LIGHT(0);
      Leddisp(HEAT_TEMP100,0);/*2015-11-29*/
      // 如果是预约，告知服务器预约手动取消 code      
      if(Connect_Service==0)
      {
        DataReturn(5);
      }
      else if(Connect_Service==1)
      {
        DelayN10ms(50);
        bFlagOK=0;
        //memset(CommRcvBuf,0,512);
        DataReturn(3);
        bFlagOK=0;
        DelayN10ms(50);
        if(bFlagOK==1)
        {
          bFlagOK=0;
          OffLineTime=OFF_LINE_TIME;
        }
        else  //第二次
        {
          bFlagOK=0;
          DataReturn(3);
          bFlagOK=0;
          DelayN10ms(50);
          if(bFlagOK==1)
          {
            bFlagOK=0;
            OffLineTime=OFF_LINE_TIME;
          }
        }       
      }
    }    
    
    ADConverData();   
  
    if(EnSendHubState==1 ) //水壶放到底座上2.5S后发送状态至服务器
    {
      if(Connect_Service==0)
      {
        EnSendHubState=0;
        DataReturn(5);
      }            
      else if(WifiLight==1)
      {
        EnSendHubState=0;
        DataReturn(3);
        bFlagOK=0;
        DelayNum10ms=200;
        while(bFlagOK==0)
        {
          if(DelayNum10ms==0)
            break;
        }
        DelayNum10ms=0;
        if(bFlagOK==1)
        {
           bFlagOK=0;
           OffLineTime=OFF_LINE_TIME;
        }
        else   //第二次
        {
          bFlagOK=0;
          DataReturn(3);
          DelayNum10ms=200;
          while(bFlagOK==0)
          {
            if(DelayNum10ms==0)
              break;
          }
          DelayNum10ms=0;
          if(bFlagOK==1)
          {
            bFlagOK=0;
            OffLineTime=OFF_LINE_TIME;
          }
        }        
      }
    }        
    /* 3、Get service request   */  
    if(Connect_Service==1)
    {
      if(RcvokTime<30 && WifiLight==0)       // success connect to app service
      {
        Leddisp(WIFI_KEY,1);
        WifiLight=1;
      }        
      else if(RcvokTime>60 && WifiLight==1) // disconnect with app service
      {
        Leddisp(WIFI_KEY,0);
        WifiLight=0;
      }
      
      if(TimeSendHubState==0)
        Tel_scan();
    }
    /* 4、与服务器断开连接。超时 重启wifi模块*/
    if(OffLineTime==0 && Connect_Service==1)           
    {
      Restart_WifiMode();
      restartcount++;
    }
    if(Connect_Service>1)  
    {
      NVIC_SystemReset();      // 复位 
    }
     
    if(Telecontrol==1 || Keycontrol==1)
      App_OnLine=1;
        
    /* 5、Heating the water when enable heat & teapot is hub Through Keyboard operation  */
    Heat_Offline();    
    
    /* 6、Heating the water when enable heat & teapot is hub Through Wifi Telecontrol    */
    Heat_Online();      
    
    /* 7、Mystate check */
    if(Mystate!=MystatePre)
    {
      MystatePre=Mystate;
      StateTime=0;
      if(Mystate==0 && Keyend==1)  //按键结束
      {
              Keyend=0;
              if(Connect_Service==0)
                DataReturn(5);
              else
              {
                  DelayN10ms(20);
                  DataReturn(3);
                  bFlagOK=0;
                  DelayNum10ms=200;
                  while(bFlagOK==0)
                  {
                    if(DelayNum10ms==0)
                      break;
                  }
                  DelayNum10ms=0;
                  if(bFlagOK==1)
                  {
                    bFlagOK=0;
                    OffLineTime=OFF_LINE_TIME;
                  }                  
                  else   //第二次
                  {
                    bFlagOK=0;
                    DataReturn(3);
                    DelayNum10ms=200;
                    while(bFlagOK==0)
                    {
                      if(DelayNum10ms==0)
                        break;
                    }
                    DelayNum10ms=0;
                    if(bFlagOK==1)
                    {
                      bFlagOK=0;
                      OffLineTime=OFF_LINE_TIME;
                    }
                  }
              }              
              //memset(PhoneID,0,37);
              memcpy(PhoneID,manual,37);
      }
    }    
    /* Uart3 receve data process */
    CommProc3();    
    /* Check Order */
    CheckOrder();    
    
    /* 空闲时，每隔1分钟更新一下水壶的状态*/
    if(UpdateState==1)
    {
      UpdateState=0;
      if(Telecontrol==0 && Keycontrol==0)
      {      
        if(Connect_Service==0)
        {
          DataReturn(5);
        }
        else
        {
            DelayN10ms(20);
            DataReturn(3);
            bFlagOK=0;
            DelayNum10ms=200;
            while(bFlagOK==0)
            {
              if(DelayNum10ms==0)
                break;
            }
            DelayNum10ms=0;
            if(bFlagOK==1)
            {
               bFlagOK=0;
               OffLineTime=OFF_LINE_TIME;
            }                  
            else   //第二次
            {
              bFlagOK=0;
              DataReturn(3);
              bFlagOK=0;
              DelayNum10ms=200;
              while(bFlagOK==0)
              {
                if(DelayNum10ms==0)
                  break;
              }
              DelayNum10ms=0;
              if(bFlagOK==1)
              {
                bFlagOK=0;
                OffLineTime=OFF_LINE_TIME;
              }
            }
          }
        }
      }    
  }
#endif
}

