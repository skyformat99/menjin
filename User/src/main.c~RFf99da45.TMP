      }
      
      else if(HeatCRC==1)  /* (3.2)、after shut dowm heat 15S ,judged the water temp*/
      {
        if(HeatCRCStart==1)
        {           
          if(WaterTemp<(Warmtemp-3))     // water temp <=Warmtemp-1
          {
            RELAY(1);
            RelayOn=1;
          }          
          if(WaterTemp>=(Warmtemp-3))
