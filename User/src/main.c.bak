        else
          WaterTemp=Warmtemp-1;        
      }
            
      else if(EnCool==1)  /*冷却*/
      {
        Mystate=4;
        if(Warm==1)
          WarmTime=TeleWarmTime*60*100;      
        
        if(WaterTemp<=Warmtemp)
        {
          EnCool=0;   
          if(Warm==0)
          {
                Speak(1);
                Finish=1;
                FinishTime=0;
              
                if(Connect_Service==0)
                  DataReturn(4);
                Mystate=0;
                StateTime=0;
                
                //RecordTable[RecordNum].endtime=;
