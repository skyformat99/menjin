                }
                
              }
              else if(Rcv_OrderEnable==1)
              {
                //存入buffer
                if(Ordernum>16)
                  Ordernum=0;
                 //存入buffer
                for(i=0;i<16;i++)
                {
                  if(strncmp(OrderTable[i].OrderID,Rcv_OrderID,16)==0)
